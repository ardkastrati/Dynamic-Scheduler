<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<TabPane fx:id="diagramPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <tabs>
    <Tab text="Untitled Tab 1" />
    <Tab fx:id="addDiagramTab" text="+">
         <content>
            <VBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label text="Choose Calculation:" />
                  <ChoiceBox fx:id="calculationBox" prefWidth="150.0" />
                  <Label text="Choose Diagram Type:" />
                  <ChoiceBox fx:id="diagramBox" prefWidth="150.0" />
                  <Button fx:id="showButton" mnemonicParsing="false" onAction="#show" text="Show" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
         </content>
      </Tab>
  </tabs>
</TabPane>
