<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<TabPane fx:id="diagramPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="290.0" prefWidth="302.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.VisualisationScene.VisualisationSceneController">
  <tabs>
    <Tab fx:id="addDiagramTab" closable="false" text="   +   ">
         <content>
            <VBox alignment="CENTER" prefHeight="226.0" prefWidth="253.0" spacing="5.0">
               <children>
                  <Label text="Choose Calculation:" />
                  <ChoiceBox fx:id="calculationBox" prefWidth="150.0" />
                  <Label text="Choose Diagram Type:" />
                  <ChoiceBox fx:id="diagramBox" prefWidth="150.0" />
                  <Button fx:id="showButton" mnemonicParsing="false" onAction="#show" text="Show" />
                  <Button fx:id="refresh" mnemonicParsing="false" onAction="#refresh" text="Refresh" />
                  <Button fx:id="cl" mnemonicParsing="false" onAction="#chooseLoc" text="Choose Local" />
                  <ProgressIndicator fx:id="refreshindicator" prefHeight="58.0" prefWidth="200.0" visible="false" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
         </content>
      </Tab>
  </tabs>
</TabPane>
